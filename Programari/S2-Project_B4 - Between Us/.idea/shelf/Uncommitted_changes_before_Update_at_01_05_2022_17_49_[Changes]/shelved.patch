<+>package salle.url.edu.business;\r\n\r\nimport salle.url.edu.persistance.PlayerDAO;\r\nimport salle.url.edu.persistance.sql.SQLPlayerDAO;\r\n\r\nimport java.io.IOException;\r\nimport java.util.ArrayList;\r\n\r\npublic class PlayersManager {\r\n\r\n    //ATRIBUTS----------------------------------------------------------------------------------------------------------\r\n\r\n    private ArrayList <Player> players;\r\n    private final PlayerDAO playerDAO;\r\n\r\n    //CONSTRUCTORS------------------------------------------------------------------------------------------------------\r\n    public PlayersManager(ArrayList<Player> players, PlayerDAO playerDAO) {\r\n        this.players = players;\r\n        this.playerDAO = playerDAO;\r\n    }\r\n    public PlayersManager() {\r\n        playerDAO = new SQLPlayerDAO();\r\n        players = playerDAO.getAllPlayers();\r\n\r\n    }\r\n\r\n    //METHODS-----------------------------------------------------------------------------------------------------------\r\n    public boolean createPlayer (String name, String email, String password){\r\n        Player player = new Player(null, false, name, email, password);\r\n\r\n        try {\r\n            playerDAO.addPlayer(player);\r\n            return true;\r\n        } catch (IOException e) {\r\n            e.printStackTrace();\r\n            return false;\r\n        }\r\n\r\n    }\r\n\r\n\r\n    /**\r\n     * Deletes a player from the database\r\n     * @param email\r\n     * @return\r\n     */\r\n\r\n    public boolean deletePlayer (String email) {\r\n\r\n        try{\r\n            playerDAO.deletePlayer(email);\r\n            return true;\r\n        } catch (IOException e) {\r\n            e.printStackTrace();\r\n            return false;\r\n        }\r\n    }\r\n\r\n    /**\r\n     * Checks if a player with a specific email is already in the Database\r\n     * @param email of the player to be checked\r\n     * @return true if is already in the database\r\n     *         false if it is not\r\n     */\r\n    public boolean checkPlayer (String email) {\r\n\r\n        try {\r\n            Player player = playerDAO.getPlayer(email);\r\n            if (player != null) {\r\n                return true;\r\n            } else{\r\n                return false;\r\n            }\r\n        } catch (IOException e) {\r\n            e.printStackTrace();\r\n        }\r\n\r\n        return false;\r\n    }\r\n}\r\n
