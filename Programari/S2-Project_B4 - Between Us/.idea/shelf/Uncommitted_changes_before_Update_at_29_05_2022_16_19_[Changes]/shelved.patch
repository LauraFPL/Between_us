atedOK = false;\n    private boolean removedOK = false;\n    private boolean accountExists = false;\n    private String userName;\n\n    private GamesController gamesController;\n\n    //private Player loggedPlayer; NO HAURIA D?HAVER UN PLAYER AQUÍ\n\n    //CONSTRUCTORS______________________________________________________________________________________________________\n    public PlayersController(MasterView masterView){\n\n        this.masterView = masterView;\n        masterView.showWindow(true);\n\n        //CONNEXIÓ AMB BUSSINESS\n        this.playersManager = new PlayersManager(new SQLPlayerDAO());\n        //INTERFICIE GRÀFICA\n\n            //Start View\n            this.startView = new StartView();\n            startView.registerController(this);\n            masterView.addView( startView, MasterView.START_VIEW);\n            masterView.changeView(MasterView.START_VIEW);\n\n\n            //Login\n            this.loginView = new LoginView();\n            masterView.addView(loginView, MasterView.LOGIN_VIEW);\n            loginView.registerController(this);\n\n\n            //registre\n            this.registerView = new RegisterView();\n            registerView.registerController(this);\n            masterView.addView( registerView, MasterView.REGISTER_VIEW);\n\n            //account view\n            this.accountView = new AccountView();\n            accountView.registerController(this);\n            masterView.addView( accountView, MasterView.ACCOUNT_VIEW);\n\n            //Settings\n            this.settingsView = new SettingsView();\n            settingsView.registerController(this);\n            masterView.addView( settingsView, MasterView.SETTINGS_VIEW);\n\n            //Warning\n            this.warningView = new WarningView();\n            warningView.registerController(this);\n            masterView.addView( warningView, MasterView.WARNING_VIEW);\n\n            //Confirm\n            this.confirmationView = new ConfirmationView();\n            this.confirmationView.showWindow(true);\n            this.confirmationView.registerController(this);\n            masterView.addView(confirmationView, MasterView.CONFIRMATION_VIEW);\n\n    }\n\n    //INTERFICIE GRÀFICA________________________________________________________________________________________________\n    @Override\n    public void actionPerformed(ActionEvent e) {\n\n        //START---------------------------------------------------------------------------------------------------------\n        if (e.getActionCommand().equals(startView.START_LEAVE_BUTTON_COMMAND)) {\n            //System.out.println(\"Start, LEAVE PRESSED\");\n            System.exit(0);\n\n        } else if (e.getActionCommand().equals(startView.START_PLAY_BUTTON_COMMAND)) {\n            //System.out.println(\"Start, PLAY TRIANG PRESSED\");\n            masterView.changeView(MasterView.LOGIN_VIEW);\n\n        }\n\n        //LOGIN---------------------------------------------------------------------------------------------------------\n        if (e.getActionCommand().equals(loginView.LOGIN_BACK_BUTTON_COMMAND)) {\n            //System.out.println(\"LOGIN, BACK PRESSED\");\n            masterView.changeView(MasterView.START_VIEW);\n\n        } else if (e.getActionCommand().equals(loginView.LOGIN_START_BUTTON_COMMAND)) {\n            //System.out.println(\"LOGIN, START PRESSED\");\n            ///* Ho commento per saltarme la verificacio i anar mes rapid\n            //System.out.println(\"password: \" + loginView.getPassword() + \" User: \" + loginView.getUserName());\n            if (playersManager.checkUserNamePlayer(loginView.getUserName())){ //Si ja està a la base de dades\n\n               if (playersManager.checkPasswordFromPlayer(loginView.getUserName(), loginView.getPassword())) { //comprovem password\n\n                    //loggedPlayer = playersManager.getPlayer(loginView.getUserName());\n\n                   //TODO: Entrar a la conta del jugador\n                   System.out.pri